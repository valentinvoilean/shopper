<!doctype html>
<!--[if lt IE 7]><html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]><html class="no-js lt-ie9 lt-ie8" lang="en"> <![endif]-->
<!--[if IE 8]><html class="no-js lt-ie9" lang="en"> <![endif]-->
<!--[if IE 9 ]><html class="ie9 no-js"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <html class="no-js"> <!--<![endif]-->
<head>
  <meta charset="utf-8" />
  <!--[if IE]><meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1' /><![endif]-->

  <title>
  {{ page_title }}{% if current_tags %} &ndash; tagged "{{ current_tags | join: ', ' }}"{% endif %}{% if current_page != 1 %} &ndash; Page {{ current_page }}{% endif %}{% unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless %}
  </title>

  {% if page_description %}
  <meta name="description" content="{{ page_description | escape }}" />
  {% endif %}

  <link rel="canonical" href="{{ canonical_url }}" />

  <meta name="viewport" content="width=device-width, initial-scale=1" />
  {{ content_for_header }}
  <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,400italic,600,600italic' rel='stylesheet' type='text/css'>
  {{ 'theme.scss.css' | asset_url | stylesheet_tag }}

  {{ 'shopify_common.js' | shopify_asset_url | script_tag }}
  {% if template contains 'customers' %}
  {{ 'customer_area.js'  | shopify_asset_url | script_tag }}
  {% endif %}
  <!-- Additional Shopify helpers that will likely be added to the global shopify_common.js some day soon. -->
  {{ 'shopify_common.js'  | asset_url | script_tag }}
  {{ 'option_selection.js' | shopify_asset_url | script_tag }}
  {{ 'vendors.js'  | asset_url | script_tag }}
</head>

<body id="{{ page_title | handle }}" class="{% if customer %}customer-logged-in {% endif %}template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}" >

  <header role="banner" class="mainHeader header">
    <div class="headerTop visible-sm visible-lg">
      <div class="container">
        <div class="currencies">
            <ul class="currencies__list">
              <li class="currencies__item is-active">€EUR</li>
              <li class="currencies__item">£GBP</li>
              <li class="currencies__item">$USD</li>
            </ul>
        </div>
        <div class="countries">
          <ul class="countries__list">
            <li class="countries__item">GB</li>
            <li class="countries__item">ES</li>
          </ul>
        </div>
        <div class="myAccount">
          Sign In / Register
        </div>
        <div class="newsletter">
          Newsletter
        </div>
      </div>
    </div>
    <div class="headerMain"></div>
    <div class="headerBottom">
      <div class="topInfo">Info</div>
      <div class="breadcrumb">Navigation</div>
    </div>
  </header>

  <main role="main">
      {{ content_for_layout }}
    <div class="container main">
      <div class="row">
        <div class="test col-xs-1"><div class="testBackground"></div></div>
        <div class="test col-xs-1"><div class="testBackground"></div></div>
        <div class="test col-xs-1"><div class="testBackground"></div></div>
        <div class="test col-xs-1"><div class="testBackground"></div></div>
        <div class="test col-xs-1"><div class="testBackground"></div></div>
        <div class="test col-xs-1"><div class="testBackground"></div></div>
        <div class="test col-xs-1"><div class="testBackground"></div></div>
        <div class="test col-xs-1"><div class="testBackground"></div></div>
        <div class="test col-xs-1"><div class="testBackground"></div></div>
        <div class="test col-xs-1"><div class="testBackground"></div></div>
        <div class="test col-xs-1"><div class="testBackground"></div></div>
        <div class="test col-xs-1 order-last"><div class="testBackground"></div></div>
      </div>
    </div>
  </main>

  <footer role="contentinfo"></footer>

{{ 'main.bundle.js'  | asset_url | script_tag }}
</body>
</html>
